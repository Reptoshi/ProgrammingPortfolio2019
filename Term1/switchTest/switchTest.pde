int grade = int(random(93, 100));

switch(grade) {
case 100:
  println(grade + " Assign letter grade A.");
  break;
case 99:
  println(grade + " Assign letter grade A.");
  break;
case 98:
  println(grade + " Assign letter grade A.");
  break;
case 97:
  println(grade + " Assign letter grade A.");
  break;
case 96:
  println(grade + " Assign letter grade A.");
  break;
case 95:
  println(grade + " Assign letter grade A.");
  break;
case 94:
  println(grade + " Assign letter grade A.");
  break;
case 93:
  println(grade + " Assign letter grade A-.");
  break;
}
